                                       <!-- Contact Form -->
                    <form action="/OnePunchMan/guardar5" method="post">
                    <div class="form-group">
								<input type="text" style="visibility: hidden" class="form-control-input" id="id"
									name="id" required value="${juegos.id}"/>
							</div>
                        <div class="form-group">
								<input type="text" class="form-control-input" id="nombrejuego"
									name="nombrejuego" required value="${juegos.nombrejuego}"> <label
									class="label-control">Nombre del juego</label>
								<div class="help-block with-errors"></div>
							</div>
							<div class="form-group">
								<input type="text" class="form-control-input" id="empresa"
									name="empresa" required value="${juegos.empresa}"> <label class="label-control">Empresa</label>
								<div class="help-block with-errors"></div>
							</div>
							<div class="form-group">
								<input type="text" class="form-control-input" id="rival"
									name="rival" required value="${juegos.rival}"> <label class="label-control">Rival</label>
								<div class="help-block with-errors"></div>
							</div>

							<div class="form-group">
								<select class="form-control-select" id="resultado"
									name="resultado" required value="${juegos.resultado}">
									<option class="select-option" value="" disabled selected>Resultado
										Batalla</option>
									<option class="select-option" value="Ganador">Ganador</option>
									<option class="select-option" value="Derrotado">Derrotado</option>
								</select>
								<div class="help-block with-errors"></div>
							</div>
							<div class="form-group">
								<select class="form-control-select" id="favorito"
									name="favorito" required value="${juegos.favorito}">
									<option class="select-option" value="" disabled selected>Favorito</option>
									<option class="select-option" value="Si">Si</option>
									<option class="select-option" value="No">No</option>
								</select>
								<div class="help-block with-errors"></div>
							</div>

							<div class="form-group">
								<button type="submit" class="form-control-submit-button2">Editar</button>
							</div>
							<div class="form-message">
								<div id="cmsgSubmit" class="h3 text-center hidden"></div>
							</div>
						</div>
                        
                    </form>
